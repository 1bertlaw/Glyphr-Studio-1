{
  "name": "Glyphr-Studio",
  "version": "0.4.0",
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-nodeunit": "~0.4.1",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-cssmin": "^0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattlag/Glyphr-Studio.git"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": false,
    "eqeqeq": true,
    "evil": false,
    "forin": true,
    "freeze": true,
    "latedef": false,
    "newcap": true,
    "nonbsp": true,
    "plusplus": false,
    "strict": false,
    "undef": false,
    "unused": false,
    "debug": true,
    "globalstrict": true,
    "browser": true,
    "devel": true,
    "predef": [
      "Coord",
      "Path",
      "PathPoint",
      "Shape",
      "LinkedShape",
      "Char",
      "GlyphrProject",
      "Guide"
    ]
  }
}
